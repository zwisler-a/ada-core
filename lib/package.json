  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "tbp": "npm run test && npm run build && npm publish --access public",
    "test": "jest"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "",
  "license": "ISC",
  "dependencies": {
